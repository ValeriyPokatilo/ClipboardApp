CREATE TABLE DbItem (
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    title TEXT NOT NULL,
    value TEXT NOT NULL
);

selectAll:
SELECT * FROM DbItem;

selectById:
SELECT * FROM DbItem WHERE id = ?;

insertItem:
INSERT INTO DbItem(title, value)
VALUES (?, ?);

deleteById:
DELETE FROM DbItem WHERE id = ?;

editItem:
UPDATE DbItem
SET title = ?, value = ?
WHERE id = ?;